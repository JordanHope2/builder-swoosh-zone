# JobEqual Platform Environment Variables
# This is an example file. Copy it to .env and fill in your actual values.

# Development
NODE_ENV=development
PORT=8080

# Supabase Configuration
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Database
DATABASE_URL=

# Authentication
JWT_SECRET=
NEXTAUTH_SECRET=

# Email Service (for notifications)
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=

# Stripe (for payments)
STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# File Storage
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=

# External APIs
OPENAI_API_KEY=

# Monitoring & Analytics
SENTRY_DSN=

# Frontend URLs
VITE_APP_URL=http://localhost:8080
VITE_API_URL=http://localhost:8080/api

# Adzuna API
ADZUNA_APP_ID=
ADZUNA_API_KEY=

# Cloudflare R2
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_BUCKET_NAME=
R2_PUBLIC_URL=

# Builder.io
VITE_BUILDER_API_KEY=

# Google OAuth
# Note: These are configured in your Supabase dashboard, not directly in the app.
# They are included here for completeness.
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
